<tr id="<%= dom_id product %>">
  <td><%= product.name %></td>
  <td><%= product.description %></td>
  <td><%= product.price %></td>
  <td><%= image_tag product.image_url ? product.image_url : '', style: "height: 100px; object-fit: contain" %></td>
  <td><%= product.quantity %></td>
  <td><%= product.category %></td>
</tr>